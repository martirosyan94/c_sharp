<Window x:Class="RegistrationSystemWPF.AccountWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RegistrationSystemWPF"
        mc:Ignorable="d"
        Title="AccountWindow" Height="450" Width="800">
    <StackPanel>
        <Label Width="100">Name</Label>
        <TextBlock Width="100" Margin="10" Name="txtBoxName"></TextBlock>
        <Label Width="100">Email </Label>
        <TextBlock Width="100" Margin="10" Name="txtBoxEmail"></TextBlock>    
    </StackPanel>
</Window>
